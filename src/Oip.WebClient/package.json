{
  "name": "oip",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "prestart": "node aspnetcore-https",
    "start": "run-script-os",
    "start:windows": "ng serve oip --port 50002 --ssl --ssl-cert \"%APPDATA%\\ASP.NET\\https\\%npm_package_name%.pem\" --ssl-key \"%APPDATA%\\ASP.NET\\https\\%npm_package_name%.key\"",
    "start:default": "ng serve oip --port 50002 --ssl --ssl-cert $HOME/.aspnet/https/${npm_package_name}.pem --ssl-key $HOME/.aspnet/https/${npm_package_name}.key",
    "start:rtds": "run-script-os",
    "start:rtds:windows": "ng serve oip-rtds --port 50003 --ssl --ssl-cert \"%APPDATA%\\ASP.NET\\https\\%npm_package_name%.pem\" --ssl-key \"%APPDATA%\\ASP.NET\\https\\%npm_package_name%.key\"",
    "start:rtds:default": "ng serve oip-rtds --port 50003 --ssl --ssl-cert $HOME/.aspnet/https/${npm_package_name}.pem --ssl-key $HOME/.aspnet/https/${npm_package_name}.key",
    "i18n:oip:extract": "ngx-translate-extract --input ./projects/oip-common/src ./projects/oip/src --output ./projects/oip/src/assets/i18n/{en,ru}.json --clean --format namespaced-json",
    "serve:dist": "concurrently \"serve dist/oip -l 5000 -s\" ",
    "generate-api": "NODE_TLS_REJECT_UNAUTHORIZED=0 npx swagger-typescript-api generate -p https://localhost:5002/swagger/v1/swagger.json -o ./projects/oip/src/api --clean-output --unwrap-response-data --extract-enums --extract-responses --extract-request-body --extract-request-params --modular --module-name-first-tag --t ./templates",
    "generate-base-api": "NODE_TLS_REJECT_UNAUTHORIZED=0 npx swagger-typescript-api generate -p https://localhost:5002/swagger/base/swagger.json -o ./projects/oip-common/src/api --clean-output --unwrap-response-data --extract-enums --extract-responses --extract-request-body --extract-request-params --modular --module-name-first-tag --t ./templates",
    "generate-rtds-api": "NODE_TLS_REJECT_UNAUTHORIZED=0 npx swagger-typescript-api generate -p https://localhost:50003/swagger/v1/swagger.json -o ./projects/oip-rtds/src/api --clean-output --unwrap-response-data --extract-enums --extract-responses --extract-request-body --extract-request-params --modular --module-name-first-tag --t ./templates",
    "build": "ng build oip",
    "build:oip": "ng build oip",
    "build:oip-rtds": "ng build oip-rtds",
    "build:common": "ng build oip-common",
    "test": "ng test",
    "lint": "ng lint --fix || npx prettier --write projects/",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.3.2",
    "@angular/common": "^20.3.2",
    "@angular/compiler": "^20.3.2",
    "@angular/core": "^20.3.2",
    "@angular/forms": "^20.3.2",
    "@angular/platform-browser": "^20.3.2",
    "@angular/platform-browser-dynamic": "^20.3.2",
    "@angular/router": "^20.3.2",
    "@ngx-translate/core": "^16.0.4",
    "@ngx-translate/http-loader": "^16.0.1",
    "@primeng/themes": "^20.2.0",
    "angular-auth-oidc-client": "^20.0.2",
    "chart.js": "4.4.2",
    "primeclt": "^0.1.5",
    "primeflex": "^4.0.0",
    "primeicons": "^7.0.0",
    "primeng": "^20.2.0",
    "rxjs": "~7.8.2",
    "tailwindcss-primeui": "^0.6.1",
    "tslib": "^2.8.1",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.3.3",
    "@angular/cli": "^20.3.3",
    "@angular/compiler-cli": "^20.3.2",
    "@types/jasmine": "~5.1.9",
    "@vendure/ngx-translate-extract": "^10.1.0",
    "angular-eslint": "20.3.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.2.1",
    "jasmine-core": "~5.4.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "postcss": "^8.4.49",
    "prettier": "^3.0.0",
    "run-script-os": "^1.1.6",
    "swagger-typescript-api": "^13.2.3",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.3",
    "typescript-eslint": "8.44.1"
  }
}
