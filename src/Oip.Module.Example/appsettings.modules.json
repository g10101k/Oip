{
  "ModuleFederation": {
    "Name": "Oip.Module.Example",
    "BaseUrl": "http://localhost:50001/",
    "RemoteEntry": "http://localhost:50001/remoteEntry.js",
    "ExportModules": [
      {
        "ExposedModule": "./OtherModule",
        "DisplayName": "OtherModule",
        "RoutePath": "other-module",
        "NgModuleName": "OtherModule",
        "SourcePath": "./src/app/modules/other-module/other.module.ts"
      },
      {
        "ExposedModule": "./WeatherModule",
        "DisplayName": "Weather",
        "RoutePath": "weather",
        "NgModuleName": "WeatherModule",
        "SourcePath": "./src/app/modules/weather/weather.module.ts"
      }
    ]
  }
}
