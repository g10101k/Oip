    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^17.0.3",
    "ngx-build-plus": "^17.0.0",
    "protractor": "~7.0.0",
    "serve": "^14.2.1",
    "ts-node": "~10.9.2",
    "tslint": "~6.1.3",
    "typescript": "^5.2.2",
    "word-wrap": "^1.2.5"
  }
